# Core Supabase configuration
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_PROJECT_REF=your-project-ref

# Agent + LLM keys
OPENAI_API_KEY=your-openai-key
ANTHROPIC_API_KEY=optional-anthropic-key

# LangGraph + runtime options
LANGGRAPH_MODEL=gpt-4o-mini
LANGCHAIN_TRACING_V2=false

# Optional storage buckets
SUPABASE_STORAGE_BUCKET=model-artifacts

# Defaults
DEFAULT_SYMBOL="NVDA"
DEFAULT_ISSUER_CIK=""

# SEC / EDGAR
SEC_USER_AGENT="Your Name (email or site)"

# Market data (price/momentum)
VENDOR_API_KEY=""

# Alert sink (optional)
ALERT_WEBHOOK_URL=""
ALERT_EMAIL=""

# Prefect orchestration
PREFECT_API_URL=http://127.0.0.1:4200/api
PREFECT_UI_URL=http://127.0.0.1:4200
PREFECT_PROFILE=agentic-local
PREFECT_API_KEY=optional-prefect-key

# Observability / OpenTelemetry
# Set OTEL_TRACES_EXPORTER=otlp with a collector listening on the endpoint below.
OTEL_TRACES_EXPORTER=otlp
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318
# Optional: override the default service name emitted in spans
# OTEL_SERVICE_NAME=agentic-quant-dev
